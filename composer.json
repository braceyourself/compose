{
    "name": "braceyourself/compose",
    "description": "Initialize your application using docker and docker compose",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Braceyourself\\Compose\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "braceyourself",
            "email": "ethanabrace@gmail.com"
        }
    ],
    "require": {

        "symfony/yaml": "v7.1.0-BETA1",
        "illuminate/support": "^10|^11",
        "illuminate/console": "^10|^11",
        "laravel/prompts": "^0.1"
    },
    "bin": [
        "bin/compose"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Braceyourself\\Compose\\ComposeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "orchestra/testbench": "^9.1"
    }
}
